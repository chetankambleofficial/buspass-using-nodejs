CREATE TABLE BUSPASS(
    NAME VARCHAR(20),
    FATHER_NAME VARCHAR(20),
    MOBILE_NO VARCHAR(15),
    DOB VARCHAR(20),
    COLLEGE VARCHAR(20),
    GENDER VARCHAR(10),
    ADHAAR VARCHAR(12),
    CAT VARCHAR(20),
    FROM_LOCATION VARCHAR(20), -- Changed from "FROM" to "FROM_LOCATION"
    TO_LOCATION VARCHAR(20),
    CHANGE VARCHAR(20),
    TODATE VARCHAR(20)
);